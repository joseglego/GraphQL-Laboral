{
  "name": "graphqllaboral",
  "version": "1.0.0",
  "description": "API GraphQL para informacion de ofertas laborales",
  "main": "server.js",
  "scripts": {
    "test": "jest",
    "test:coverage": "jest --coverage",
    "start": "node server.js",
    "start:dev": "npm run build:dev",
    "build:dev": "nodemon 'src/server.ts' --exec 'ts-node' src/server.ts -e ts,graphql",
    "build": "tsc -p . && ncp src/schema dist/schema",
    "lint": "eslint \"*/**/*.ts\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/raicerk/graphqllaboral.git"
  },
  "keywords": [
    "graphql",
    "js"
  ],
  "author": "Juan Mora",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/raicerk/graphqllaboral/issues"
  },
  "homepage": "https://github.com/raicerk/graphqllaboral#readme",
  "dependencies": {
    "@types/mongodb": "^3.5.25",
    "apollo-server": "^2.16.1",
    "graphql": "^15.3.0",
    "mongodb": "^3.6.0",
    "ncp": "^2.0.0",
    "outliers": "0.0.3"
  },
  "devDependencies": {
    "@types/graphql": "^14.5.0",
    "@types/jest": "^26.0.9",
    "@types/node": "^14.0.27",
    "@typescript-eslint/eslint-plugin": "^3.8.0",
    "@typescript-eslint/parser": "^3.8.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "graphql-import": "^1.0.2",
    "graphql-import-node": "0.0.4",
    "jest": "^26.2.2",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "prettier-eslint": "^11.0.0",
    "ts-jest": "^26.1.4",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  }
}
